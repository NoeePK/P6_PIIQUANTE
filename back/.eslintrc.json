{
    "env": {
        "browser": true,
        "node": true,
        "amd": true,
        "es2021": true
    },
    "globals": {
        "require": true,
        "process": true,
        "next": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": "off"
    }
}